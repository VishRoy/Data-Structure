class HashTable{
  constructor(){
    this.size = 1000;
    this.buckets = Array(1000).fill(null);
  }

  hash(key){
    let hash = 0
    for(const char of key){
      hash += char.charCodeAt(0)
    }
    return hash % this.size;
  }

  set(key,value){
    let keyHash = this.hash(key)
    this.buckets[keyHash] = value
  }

  get(key){
    let keyHash = this.hash(key)
    return this.buckets[keyHash]
  }
}
